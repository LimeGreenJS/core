{
  "name": "limegreenjs",
  "version": "1.0.0",
  "private": true,
  "description": "A cloud service to help developing React/GraphQL apps",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=${NODE_ENV:-'production'} node src/server",
    "start-forever": "forever -c 'npm start' ./",
    "start-dev": "NODE_ENV=development npm start",
    "prewebpack": "rm -rf build/client",
    "webpack": "webpack --config src/server/webpack-prod-config.js",
    "test": "npm run eslint",
    "eslint": "eslint src"
  },
  "author": "Daishi Kato",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.42",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.42",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@babel/preset-react": "^7.0.0-beta.42",
    "@babel/runtime": "^7.0.0-beta.42",
    "apollo-cache-inmemory": "^1.1.11",
    "apollo-client": "^2.2.7",
    "apollo-link-http": "^1.5.3",
    "babel-loader": "^8.0.0-beta.2",
    "config": "^1.30.0",
    "file-loader": "^1.1.11",
    "forever": "^0.15.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "html-webpack-plugin": "^2.30.1",
    "koa": "^2.5.0",
    "koa-favicon": "^2.0.1",
    "koa-logger": "^3.2.0",
    "koa-send": "^4.1.3",
    "koa-static": "^4.0.2",
    "koa-webpack": "^2.0.3",
    "linkfs": "^2.1.0",
    "lru-cache": "^4.1.2",
    "material-ui": "^1.0.0-beta.38",
    "material-ui-icons": "^1.0.0-beta.36",
    "memfs": "^2.8.0",
    "raw-loader": "^0.5.1",
    "react": "^16.2.0",
    "react-apollo": "^2.1.0",
    "react-compose-state": "^1.1.0",
    "react-dom": "^16.2.0",
    "react-infinite-scroller": "^1.1.3",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "unionfs": "^3.0.2",
    "unzipper": "^0.8.11",
    "webpack": "^3.11.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0"
  },
  "eslintConfig": {
    "extends": [
      "airbnb"
    ],
    "rules": {
      "no-console": 0,
      "global-require": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/prop-types": 0,
      "no-param-reassign": 0
    }
  }
}
